{
  "title": "Dictation to Zoom",
  "rules": [
    {
      "description": "Dictation key (F5 action) to Zoom mute/unmute",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f5",
            "modifiers": {
              "optional": [
                "caps_lock"
              ]
            }
          },
          "to": [
            {
              "key_code": "f5",
              "modifiers": [
                "left_shift",
                "left_command",
                "left_control",
                "left_option"
              ]
            }
          ]
        }
      ]
    }
  ]
}
